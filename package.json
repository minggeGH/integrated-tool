{
  "name": "integrated-tool",
  "private": false,
  "version": "0.0.2",
  "description": "A Quasar Project",
  "productName": "集成工具",
  "author": "luxrg <136412296@qq.com>",
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "node quasar build",
    "build:android": "quasar build -m capacitor -T android --ide",
    "build:android:release": "cd android && .\\gradlew.bat :app:assembleRelease",
    "build:lib": "quasar build",
    "vite:build": "vite build",
    "start": "node server/app.js",
    "nodemon": "nodemon server/app.js",
    "addAndroid": "npx cap add android",
    "copyAndroid": "npx cap copy android",
    "sync": "npx cap sync",
    "openAndroid": "npx cap open android",
    "open-android": "quasar dev -m capacitor -T android",
    "capacitor-icons": "icongenie generate -m capacitor -i public/icons/favicon-96x96.png",
    "spa-icon": "icongenie generate -m spa -i public/logo.png"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@capacitor/android": "^6.1.2",
    "@capacitor/app-launcher": "^6.0.2",
    "@capacitor/camera": "^6.0.2",
    "@capacitor/cli": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/device": "^6.0.1",
    "@capacitor/network": "^6.0.2",
    "@quasar/extras": "^1.16.4",
    "axios": "^1.6.8",
    "chroma-js": "^2.4.2",
    "dotenv": "^16.4.5",
    "leaflet": "^1.9.4",
    "pinia": "^2.2.6",
    "quasar": "^2.8.0",
    "sharp": "^0.33.5",
    "unplugin-auto-import": "^0.18.3",
    "v-viewer": "^3.0.13",
    "viewerjs": "^1.11.6",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12",
    "weixin-js-sdk": "^1.6.5",
    "weixin-webview-jssdk": "^0.0.2"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.214",
    "@iconify/tailwind": "^1.1.1",
    "@quasar/app-vite": "^1.4.3",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "daisyui": "^4.7.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.4.35",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.4.1",
    "vite-plugin-libcss": "^1.1.1"
  },
  "engines": {
    "node": "^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}